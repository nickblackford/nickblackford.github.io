    "from tqdm import tqdm\n",
    "import pandas as pd\n",
    "from IPython.display import display\n",
    "\n",
    "# Token counter\n",
    "def count_tokens(text):\n",
    "    return len(model.tokenize(text))\n",
    "\n",
    "# Prompt template\n",
    "def mistral_prompt(line, hard_skills, soft_skills):\n",
    "    example_rewrite = (\n",
    "        \"* Original: Improved model reliability and time-to-production by automating end-to-end pipelines in collaboration with data engineering teams.\\n\"\n",
    "        \"* Rewritten: Improved model reliability and reduced time-to-production 25% by collaborating with data engineering on automated pipelines using Python and pandas.\"\n",
    "    )\n",
    "\n",
    "    return (\n",
    "        \"You are an expert resume writer specializing in tech resumes for data professionals applying to FAANG-level companies. \"\n",
    "        \"Your task is to rewrite the following resume bullet point for a **skilled data scientist**. Use a **concise**, **impact-oriented**, and **technical tone**. \"\n",
    "        \"Inject relevant missing skills **only if they enhance clarity or alignment**. Avoid generic filler.\\n\\n\"\n",
    "        f\"Relevant skills to consider: {', '.join(hard_skills + soft_skills)}\\n\\n\"\n",
    "        f\"{example_rewrite}\\n\\n\"\n",
    "        f\"* Original: {line}\\n\"\n",
    "        f\"* Rewritten:\"\n",
    "    )\n",
    "\n",
    "\n",
    "# Storage\n",
    "revised_all = []\n",
    "\n",
    "for jd_name, jd_text in tqdm(job_descriptions.items()):\n",
    "    if jd_name not in final_skill_matches:\n",
    "        continue\n",
    "    \n",
    "    skills = final_skill_matches[jd_name]\n",
    "    hard_skills = skills[\"hard_skills\"]\n",
    "    soft_skills = skills[\"soft_skills\"]\n",
    "\n",
    "    for line in resume_lines:\n",
    "        hard = [s for s in hard_skills if s.lower() not in line.lower()][:2]\n",
    "        soft = [s for s in soft_skills if s.lower() not in line.lower()][:2]\n",
    "\n",
    "        if not hard and not soft:\n",
    "            revised_all.append({\n",
    "                \"Job Description\": jd_name,\n",
    "                \"Original\": line,\n",
    "                \"Rewritten\": line,\n",
    "                \"Note\": \"No rewrite needed\"\n",
    "            })\n",
    "            continue\n",
    "\n",
    "        prompt = mistral_prompt(line, hard, soft)\n",
    "\n",
    "        if count_tokens(prompt) + 128 > 512:\n",
    "            revised_all.append({\n",
    "                \"Job Description\": jd_name,\n",
    "                \"Original\": line,\n",
    "                \"Rewritten\": line,\n",
    "                \"Note\": \"Skipped due to token overflow\"\n",
    "            })\n",
    "            continue\n",
    "\n",
    "        try:\n",
    "            response = model(prompt, max_new_tokens=128)\n",
    "            clean_resp = response.strip().split(\"\\n\")[0].strip()\n",
    "        except Exception as e:\n",
    "            clean_resp = f\"Error: {e}\"\n",
    "\n",
    "        revised_all.append({\n",
    "            \"Job Description\": jd_name,\n",
    "            \"Original\": line,\n",
    "            \"Rewritten\": clean_resp,\n",
    "            \"Note\": \"Skills injected\" if clean_resp != line else \"Minimal change\"\n",
    "        })"
