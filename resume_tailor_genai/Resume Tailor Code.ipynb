    "# Resume Preprocessing\n",
    "def extract_job_bullets(resume_text):\n",
    "    lines = resume_text.splitlines()\n",
    "    keep = []\n",
    "    keep_block = False\n",
    "\n",
    "    for line in lines:\n",
    "        stripped = line.strip()\n",
    "\n",
    "        # Start keeping when we hit WORK EXPERIENCE\n",
    "        if \"WORK EXPERIENCE\" in stripped:\n",
    "            keep_block = True\n",
    "            continue\n",
    "        elif \"TECHNICAL SKILLS\" in stripped:\n",
    "            break  # Stop at end of work history\n",
    "\n",
    "        if keep_block and (stripped.startswith(\"*\") or \" - \" in stripped):\n",
    "            keep.append(stripped)\n",
    "\n",
    "    return keep\n",
    "\n",
    "# Load cleaned resume\n",
    "with open(\"data/resumes/resume_nick_blackford.txt\") as f:\n",
    "    full_resume = f.read()\n",
    "\n",
    "resume_lines = extract_job_bullets(full_resume)\n"
   ]
