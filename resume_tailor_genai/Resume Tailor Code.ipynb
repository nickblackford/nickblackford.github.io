    "\n",
    "from sentence_transformers import SentenceTransformer, util\n",
    "\n",
    "# Load Sentence-BERT model\n",
    "model = SentenceTransformer(\"all-MiniLM-L6-v2\")\n",
    "\n",
    "# Define soft/general skill concepts (expandable)\n",
    "soft_skill_concepts = [\n",
    "    \"Experimentation\",\n",
    "    \"A/B Testing\",\n",
    "    \"Understanding user behavior\",\n",
    "    \"Communicating insights\",\n",
    "    \"Storytelling with data\",\n",
    "    \"Data-driven decision making\",\n",
    "    \"Cross-functional collaboration\",\n",
    "    \"Product thinking\",\n",
    "    \"Presenting findings to stakeholders\",\n",
    "    \"Strategic analysis\",\n",
    "    \"Mentoring junior analysts\",\n",
    "    \"Translating data to action\",\n",
    "    \"Stakeholder management\",\n",
    "    \"Business acumen\",\n",
    "    \"Root cause analysis\",\n",
    "    \"Hypothesis-driven problem solving\",\n",
    "    \"Designing experiments\",\n",
    "    \"Prioritizing analyses\",\n",
    "    \"Product intuition\",\n",
    "    \"Exploratory thinking\",\n",
    "    \"Experiment prioritization\",\n",
    "    \"Leading cross-functional teams\",\n",
    "    \"Executive communication\",\n",
    "    \"Driving business outcomes with data\",\n",
    "    \"Writing documentation\"\n",
    "]\n",
    "\n",
    "\n",
    "# Embed the soft skill concepts\n",
    "soft_skill_embeddings = model.encode(soft_skill_concepts, convert_to_tensor=True)\n",
    "\n",
    "# Break job descriptions into sentences and embed them\n",
    "from nltk.tokenize import sent_tokenize\n",
    "import nltk\n",
    "nltk.download('punkt')\n",
    "\n",
    "# Apply to each job description\n",
    "semantic_matches = {}\n",
    "\n",
    "for jd_name, jd_text in cleaned_jds.items():\n",
    "    sentences = sent_tokenize(jd_text)\n",
    "    sentence_embeddings = model.encode(sentences, convert_to_tensor=True)\n",
    "\n",
    "    # Compute similarity between each JD sentence and each soft skill concept\n",
    "    cosine_scores = util.cos_sim(sentence_embeddings, soft_skill_embeddings)\n",
    "\n",
    "    matched_skills = set()\n",
    "    threshold = 0.6 \n",
    "\n",
    "    for i, sentence in enumerate(sentences):\n",
    "        for j, concept in enumerate(soft_skill_concepts):\n",
    "            if cosine_scores[i][j] >= threshold:\n",
    "                matched_skills.add(concept)\n",
    "\n",
    "    semantic_matches[jd_name] = matched_skills\n",
    "\n",
    "# Print semantic soft skill matches\n",
    "for jd_name, matches in semantic_matches.items():\n",
    "    print(f\"\\n Soft Skill Matches for {jd_name}:\")\n",
    "    print(sorted(matches))\n"
   ]
