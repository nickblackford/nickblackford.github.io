    "# Load Gemini\n",
    "import google.generativeai as genai\n",
    "\n",
    "genai.configure(api_key=\"secret")\n",
    "\n",
    "\n",
    "# Initialize the model\n",
    "model = genai.GenerativeModel(\"gemini-1.5-flash\")\n",
    "\n",
    "# Read sample resume lines \n",
    "with open(\"data/sample_bullets.txt\", \"r\") as file:\n",
    "    sample_lines = [line.strip() for line in file.readlines() if line.strip()]\n",
    "\n",
    "# Prompt template\n",
    "def rewrite_prompt_with_jd(bullet, job_description):\n",
    "    return (\n",
    "        \"You are rewriting a resume bullet to better align with the following job description:\\n\\n\"\n",
    "        f\"{job_description}\\n\\n\"\n",
    "        \"Task: Rewrite the bullet below to be more concise, results-driven, and aligned to the job. \"\n",
    "        \"You must return exactly one improved version of the bullet. Do not include multiple options, suggestions, or commentary.\\n\\n\"\n",
    "        f\"Resume bullet: {bullet}\\n\\n\"\n",
    "        \"Return only the rewritten bullet, and nothing else.\"\n",
    "    )\n",
    "\n",
    "\n",
    "# Load meta job description\n",
    "with open(\"data/job_descriptions/meta.txt\", \"r\") as f:\n",
    "    meta_jd = f.read().strip()\n",
    "\n",
    "# Store rewrites\n",
    "gemini_rewrites = []\n",
    "\n",
    "for line in sample_lines:\n",
    "    prompt = rewrite_prompt_with_jd(line, meta_jd)\n",
    "    try:\n",
    "        response = model.generate_content(prompt)\n",
    "        full_response = response.text.strip()\n",
    "\n",
    "        # Clean Gemini's output: only keep the first bullet-like sentence\n",
    "        first_line = full_response.split(\"\\n\")[0]\n",
    "        if first_line.startswith(\">\"):\n",
    "            cleaned = first_line[1:].strip()  # remove leading '>'\n",
    "        else:\n",
    "            cleaned = first_line.strip()\n",
    "\n",
    "    except Exception as e:\n",
    "        cleaned = f\"Error: {e}\"\n",
    "    \n",
    "    gemini_rewrites.append({\n",
    "        \"Original Bullet\": line,\n",
    "        \"Gemini Rewrite\": cleaned\n",
    "    })\n",
    "\n",
    "\n",
    "pd.set_option(\"display.max_colwidth\", None)\n",
    "pd.DataFrame(gemini_rewrites)\n"
   ]
