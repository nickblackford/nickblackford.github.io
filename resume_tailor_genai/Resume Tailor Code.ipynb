    "\n",
    "import pandas as pd\n",
    "# Load skill keywords from CSV\n",
    "skill_df = pd.read_csv(\"data/ds_ml_skill_keywords.csv\")\n",
    "skill_keywords = set(skill_df[\"Skill\"].dropna().str.strip().str.lower())\n",
    "\n",
    "def extract_skills_from_text(text, skill_set):\n",
    "    \"\"\"\n",
    "    Returns a set of matched skills from the text, based on keyword presence (case-insensitive).\n",
    "    \"\"\"\n",
    "    found_skills = set()\n",
    "    text_lower = text.lower()\n",
    "    for skill in skill_set:\n",
    "        if re.search(rf'\\b{re.escape(skill)}\\b', text_lower):\n",
    "            found_skills.add(skill)\n",
    "    return found_skills\n",
    "\n",
    "# Extract skills from resume\n",
    "resume_name = list(cleaned_resumes.keys())[0]\n",
    "resume_text = cleaned_resumes[resume_name]\n",
    "resume_skills = extract_skills_from_text(resume_text, skill_keywords)\n",
    "\n",
    "# Extract skills from all job descriptions\n",
    "jd_skills_map = {}\n",
    "for jd_name, jd_text in cleaned_jds.items():\n",
    "    jd_skills_map[jd_name] = extract_skills_from_text(jd_text, skill_keywords)\n",
    "\n",
    "# Preview output\n",
    "print(f\" Skills found in resume ({resume_name}):\\n\", sorted(resume_skills))\n",
    "\n",
    "for jd_name, skills in jd_skills_map.items():\n",
    "    print(f\"\\n Skills in job description ({jd_name}):\")\n",
    "    print(sorted(skills))\n"
   ]
