    "import re\n",
    "\n",
    "def clean_text(text):\n",
    "    \"\"\"\n",
    "    Cleans resume or job description text:\n",
    "    - Collapses excessive newlines and spaces\n",
    "    - Removes inconsistent spacing around punctuation\n",
    "    - Keeps paragraph structure for readability\n",
    "    \"\"\"\n",
    "    text = re.sub(r'\\r', '', text)  # Remove carriage returns\n",
    "    text = re.sub(r'\\n{2,}', '\\n\\n', text)  # Collapse multiple newlines to paragraph breaks\n",
    "    text = re.sub(r' +', ' ', text)  # Collapse multiple spaces\n",
    "    text = re.sub(r'\\s+([.,:;!?])', r'\\1', text)  # Fix space before punctuation\n",
    "    text = text.strip()\n",
    "    return text\n",
    "\n",
    "# Apply cleaning to all resumes and job descriptions\n",
    "cleaned_resumes = {k: clean_text(v) for k, v in resumes.items()}\n",
    "cleaned_jds = {k: clean_text(v) for k, v in job_descriptions.items()}\n",
    "\n",
    "# Preview cleaned resume\n",
    "resume_name = list(cleaned_resumes.keys())[0]\n",
    "print(f\" Cleaned Resume: {resume_name}\\n\")\n",
    "print(cleaned_resumes[resume_name][:1000])\n"
   ]
