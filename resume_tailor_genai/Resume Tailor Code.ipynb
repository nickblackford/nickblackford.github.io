    "\n",
    "import spacy\n",
    "\n",
    "# Load small English model\n",
    "nlp = spacy.load(\"en_core_web_sm\")\n",
    "\n",
    "def extract_entities(text, labels=[\"ORG\", \"PERSON\", \"GPE\", \"PRODUCT\", \"NORP\", \"WORK_OF_ART\", \"EVENT\", \"FAC\", \"LAW\", \"LANGUAGE\", \"DATE\", \"TIME\", \"PERCENT\", \"MONEY\", \"QUANTITY\", \"ORDINAL\", \"CARDINAL\", \"LOC\", \"SKILL\"]):\n",
    "    \"\"\"\n",
    "    Extracts named entities from text using spaCy.\n",
    "    Returns a list of strings for the desired entity labels.\n",
    "    \"\"\"\n",
    "    doc = nlp(text)\n",
    "    return [ent.text for ent in doc.ents if ent.label_ in labels]\n",
    "\n",
    "# Apply to cleaned resume and JD\n",
    "resume_entities = extract_entities(cleaned_resumes[resume_name])\n",
    "jd_name = list(cleaned_jds.keys())[0]\n",
